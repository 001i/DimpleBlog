-- ----------------------------
-- Table structure for permission
-- ----------------------------
DROP TABLE IF EXISTS `permission`;
CREATE TABLE `permission`  (
  `permission_id` int(11) NOT NULL AUTO_INCREMENT COMMENT '权限的id',
  `title` varchar(64) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '权限的名称：如友链增加',
  `name` varchar(255) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '权限的名称，如：system:link:insert',
  `p_id` int(11) NULL DEFAULT NULL COMMENT '父级权限ID',
  `request_url` varchar(64) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '请求地址',
  `type` int(11) NULL DEFAULT NULL COMMENT '类型（1为目录，2为菜单，3为按钮）',
  `icon` varchar(128) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '显示的图标',
  `status` tinyint(1) NULL DEFAULT NULL COMMENT '显示或者隐藏，1表示显示，0表示隐藏',
  PRIMARY KEY (`permission_id`) USING BTREE,
  UNIQUE INDEX `permission_id`(`permission_id`) USING BTREE,
  INDEX `title`(`title`) USING BTREE,
  INDEX `name`(`name`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 132 CHARACTER SET = utf8 COLLATE = utf8_general_ci ROW_FORMAT = Dynamic;

