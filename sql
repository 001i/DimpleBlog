-- ----------------------------
-- Table structure for rotation
-- ----------------------------
DROP TABLE IF EXISTS `rotation`;
CREATE TABLE `rotation`  (
  `id` int(11) NOT NULL AUTO_INCREMENT,
  `img` varchar(256) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '图片地址\n',
  `title` varchar(128) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '显示的文字\n',
  `alt` varchar(128) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '鼠标移动上去显示的文字',
  `create_time` datetime(0) NULL DEFAULT NULL COMMENT '创建时间',
  `display` tinyint(1) NULL DEFAULT NULL COMMENT '显示1，不显示0',
  `place` int(11) NULL DEFAULT 0 COMMENT '显示的位置，0表示未指定，1表示在轮播图，2表示在时间线',
  `url` varchar(64) CHARACTER SET utf8 COLLATE utf8_general_ci NULL DEFAULT NULL COMMENT '链接地址\n',
  `target` tinyint(1) NULL DEFAULT NULL COMMENT '是否在当前窗口打开，1表示是，0表示不是',
  PRIMARY KEY (`id`) USING BTREE,
  UNIQUE INDEX `id`(`id`) USING BTREE
) ENGINE = InnoDB AUTO_INCREMENT = 6 CHARACTER SET = utf8 COLLATE = utf8_general_ci COMMENT = '轮播图' ROW_FORMAT = Dynamic;

